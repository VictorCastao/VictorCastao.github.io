function LISTAQ3(){
    console.log("Lista Q3");
    let listaStrings = [];
    let aux = document.getElementById("string").value;
    let resp = listaStrings;
    listaStrings.push(aux);
    document.getElementById("LRQ3").innerHTML = resp;
    console.log(resp);
    document.getElementById("string");

}

function Q3(){
    console.log("Lista Q3");
    let listaStrings = ["item 1", "item 2",];
    let aux = document.getElementById("string").value;
    let resp = listaStrings;
    for (i = 0; i < listaStrings.length; i ++){
        console.log(listaStrings[i].toUpperCase);
    }
    document.getElementById("RQ3").innerHTML = resp;
    console.log(resp);  
}
